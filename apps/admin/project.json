{
  "name": "admin",
  "sourceRoot": "apps/admin/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "dependsOn": [
        {
          "target": "build",
          "projects": ["@acm/api-endpoints"]
        }
      ],
      "options": {
        "command": "pnpm --filter admin dev"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter admin build"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm --filter admin lint"
      }
    }
  }
}